function deletacode()
{
enviacode("/postcode/cataqui/del_postcode_db.php","","","");
}


function startcode()
{

var code = document.getElementById("text_code_input").value; 
var range1 =   document.getElementById("text_code_range1").value;
var range2 =   document.getElementById("text_code_range2").value; 

enviacode("/postcode/cataqui/add_postcode_db.php",code,range1,range2);

}



function enviacode(comando,code,start,finish)
{
     // Create our XMLHttpRequest object explorer ou safari
    
    if (window.XMLHttpRequest) 
    { var hr = new XMLHttpRequest();}
    if (window.ActiveXObject)
    {var hr = new ActiveXObject("Microsoft.XMLHTTP");} 
    
    // Create some variables we need to send to our PHP file
    var url = comando;
    var send_data = "code=" + code + "&start=" + start + "&finish=" + finish  ; //envia servicos 
    hr.open("POST", url, true);
    // Set content type header information for sending url encoded variables in the request
    hr.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
    // Access the onreadystatechange event for the XMLHttpRequest object

    hr.onreadystatechange = function() {
	       
         if(hr.readyState == 4 && hr.status == 200) {
          //data=hr.responseText.replace( /\s/g, ""); 

          //data=hr.responseText.trim();
       
          data=hr.responseText; 
          
           alert(data);



       }
       }
       // Send the data to PHP now... and wait for response to update the status div
     hr.send(send_data); //envia codigo para ler_monitor e atualiza os arquivos de monitor.
}



</script>

